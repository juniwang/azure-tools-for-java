<?xml version="1.0" encoding="UTF-8"?>
<project name="azure-toolkit-for-intellij" default="all">
  
  
  <property file="build.properties"/>
  <!-- Uncomment the following property if no tests compilation is needed -->
  <!-- 
  <property name="skip.tests" value="true"/>
   -->
  
  <!-- Compiler options -->
  
  <property name="compiler.debug" value="on"/>
  <property name="compiler.generate.no.warnings" value="off"/>
  <property name="compiler.args" value=""/>
  <property name="compiler.max.memory" value="700m"/>
  <patternset id="ignored.files">
    <exclude name="**/*.hprof/**"/>
    <exclude name="**/*.pyc/**"/>
    <exclude name="**/*.pyo/**"/>
    <exclude name="**/*.rbc/**"/>
    <exclude name="**/*~/**"/>
    <exclude name="**/.DS_Store/**"/>
    <exclude name="**/.bundle/**"/>
    <exclude name="**/.git/**"/>
    <exclude name="**/.hg/**"/>
    <exclude name="**/.svn/**"/>
    <exclude name="**/.tox/**"/>
    <exclude name="**/CVS/**"/>
    <exclude name="**/RCS/**"/>
    <exclude name="**/SCCS/**"/>
    <exclude name="**/__pycache__/**"/>
    <exclude name="**/_svn/**"/>
    <exclude name="**/rcs/**"/>
    <exclude name="**/vssver.scc/**"/>
    <exclude name="**/vssver2.scc/**"/>
  </patternset>
  <patternset id="library.patterns">
    <include name="*.zip"/>
    <include name="*.apk"/>
    <include name="*.war"/>
    <include name="*.egg"/>
    <include name="*.ear"/>
    <include name="*.ane"/>
    <include name="*.swc"/>
    <include name="*.jar"/>
  </patternset>
  <patternset id="compiler.resources">
    <exclude name="**/?*.java"/>
    <exclude name="**/?*.form"/>
    <exclude name="**/?*.class"/>
    <exclude name="**/?*.groovy"/>
    <exclude name="**/?*.scala"/>
    <exclude name="**/?*.flex"/>
    <exclude name="**/?*.kt"/>
    <exclude name="**/?*.clj"/>
    <exclude name="**/?*.aj"/>
  </patternset>

  <!-- IDEA library jars -->
  <fileset id="idea.libraries" dir="${idea.home}">
    <include name="**/*.jar" />
  </fileset>

  <fileset id="scala.libraries" dir="${scala.plugin.home}">
    <include name="lib/scala-plugin.jar"/>
  </fileset>

  <path id="jdk.classpath.idea">
    <resources refid="idea.libraries" />
    <resources refid="scala.libraries"/>
  </path>
  
  <!-- Project Libraries -->
  
  <path id="library..cspack.classpath">
    <pathelement location="${basedir}/lib/.cspack.jar"/>
  </path>

  <path id="library.antintegration.classpath">
    <pathelement location="${idea.home}/plugins/ant/lib/antIntegration.jar"/>
  </path>

  <path id="library.services-explorer-common.classpath">
    <pathelement location="${basedir}/lib/services-explorer-common.jar"/>
  </path>
  
  <path id="library.hdinsight-node-common.classpath">
    <pathelement location="${basedir}/lib/hdinsight-node-common.jar"/>
  </path>

  <patternset id="execude.hdinsight.sample.pattern">
    <exclude name="**/hdinsight/templates/**"/>
  </patternset>

  <path id="library.azure-sdk.classpath">
    <pathelement location="${basedir}/lib/azure-sdk/azure-core-0.9.2.jar"/>
    <pathelement location="${basedir}/lib/azure-sdk/annotation-detector-3.0.4.jar"/>
    <pathelement location="${basedir}/lib/azure-sdk/applicationinsights-core-1.0.3.jar"/>
