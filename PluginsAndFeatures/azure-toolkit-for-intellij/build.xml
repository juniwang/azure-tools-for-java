    <pathelement location="${basedir}/lib/azure-sdk/commons-lang-2.6.jar"/>
    <pathelement location="${basedir}/lib/azure-sdk/commons-logging-1.1.3.jar"/>
    <pathelement location="${basedir}/lib/azure-sdk/httpclient-4.3.6.jar"/>
